{
  "name": "sumocoders/framework-core-bundle",
  "type": "symfony-bundle",
  "description": "The core bundle for our mini-framework",
  "keywords": ["Framework", "SumoCoders"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tijs Verkoyen",
      "email": "tijs@sumocoders.be"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.3.3",
    "symfony/framework-bundle": "2.3.*",
    "jms/i18n-routing-bundle": "1.*",
    "jms/translation-bundle": "1.1.*",
    "mopa/bootstrap-bundle": "3.*@dev",
    "knplabs/knp-menu-bundle": "2.0.*@dev",
    "sumocoders/framework-error-bundle": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*"
  },
  "autoload": {
    "psr-0": { "SumoCoders\\FrameworkCoreBundle": "" }
  },
  "target-dir": "SumoCoders/FrameworkCoreBundle"
}
